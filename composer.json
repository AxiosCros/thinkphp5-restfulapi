{
  "name": "axios/tpr-cms",
  "description": "base on tpr framework",
  "type": "project",
  "keywords": [
    "tpr",
    "restful",
    "tpr-cms",
    "api"
  ],
  "homepage": "https://github.com/AxiosCros/tpr-cms",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "axios",
      "email": "axioscros@aliyun.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "axios/tpr": "dev-master",
    "axios/tpr-db": "^0.2",
    "nette/php-generator": "^3.2"
  },
  "require-dev": {
    "symfony/var-dumper": "*"
  },
  "autoload": {
    "psr-4": {
